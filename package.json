{
  "name": "esteban-soubiran.site",
  "version": "2.1.1",
  "private": true,
  "description": "Portfolio d'Estéban Soubiran",
  "author": {
    "name": "Estéban Soubiran",
    "email": "e.soubiran25@gmail.com",
    "url": "https://esteban-soubiran.site"
  },
  "keywords": [
    "portfolio",
    "esteban soubiran",
    "projets",
    "personnel"
  ],
  "repository": {
    "type": "git"
  },
  "engines": {
    "node": ">=12.20.0"
  },
  "scripts": {
    "update": "bump",
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "run-p lint:all:*",
    "lint:all:eslint": "yarn lint:eslint --fix",
    "lint:all:markdownlint": "yarn lint:markdownlint --fix",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern modules/ --ext .js,.vue .",
    "lint:markdownlint": "markdownlint ./content",
    "lint:prettier": "prettier  \"**/*.{js,json,css,scss,vue,html,md}\""
  },
  "lint-staged": {
    "*.{js,json,css,vue,md}": "prettier --write",
    "*.{js,vue}": "eslint --fix",
    "*.md": "markdownlint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/content": "^1.14.0",
    "@nuxtjs/composition-api": "^0.29.3",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/robots": "^2.5.0",
    "@nuxtjs/sitemap": "^2.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "core-js": "^3.21.0",
    "nuxt": "^2.15.8",
    "nuxt-use-motion": "^2.0.0-beta.5",
    "tailwindcss": "^3.0.9",
    "vue-observe-visibility": "^1.0.0"
  },
  "devDependencies": {
    "@fabiospampinato/bump": "^2.1.0",
    "@nuxt/image": "^0.6.0",
    "@nuxt/postcss8": "^1.1.3",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "autoprefixer": "^10.4.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.4",
    "markdownlint-cli": "^0.28.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "prettier": "^2.3.2"
  }
}
